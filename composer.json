    "require": {
        "ext-pdo": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.58"
    },
    "scripts": {
        "start:linux": [
            "Composer\\Config::disableProcessTimeout",
            "bin/run-server.sh -d auto_prepend_file=\"$PWD/vendor/autoload.php\""
        ],
        "test:cs": [
            "php vendor/bin/php-cs-fixer fix --dry-run"
        ],
        "fix:cs": [
            "php vendor/bin/php-cs-fixer fix"
        ],
        "start:windows": [
            "Composer\\Config::disableProcessTimeout",
            "bin/run-server.bat"
        ],
        "start": [
            "@start:linux"
        ]
    }
}